@using BookStore.ViewModel
@model AutherVM
@section title {
	Auther
}
@section subTitle {
		Details of <span>@Model.Name</span>
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Created On</th>
            <th scope="col">Updated On</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.Id</th>
            <td>@Model.Name</td>
            <td>@Model.CreatedOn</td>
            <td>@Model.UpdatedOn</td>
        </tr>

    </tbody>
</table>
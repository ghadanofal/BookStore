@using BookStore.ViewModel
@model CategoryVM

@section title {
	categories
}
@section subTitle {
	Details
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Created On</th>
            <th scope="col">Updated On</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.Id</th>
            <td>@Model.Name</td>
            <td>@Model.CreatedOn</td>
            <td>@Model.UpdatedOn</td>
        </tr>
        
    </tbody>
</table>